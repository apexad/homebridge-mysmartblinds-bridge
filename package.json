  "version": "3.0.1",
  "description": "Homebrige plugin for mysmartblinds (using hardware bridge)",
  "keywords": [
    "homebridge-plugin",
    "mysmartblinds",
    "tiltsmarthome",
    "category-hubs",
    "category-climate"
  ],
  "engines": {
    "homebridge": ">=1.1.0",
    "node": ">=14.15.0"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "prompt": "^1.0.0",
