    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.5",
    "@types/request-promise": "^4.1.46",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^8.44.0",
    "homebridge": "^1.2.2",
    "nodemon": "^2.0.5",
    "rimraf": "^5.0.0",
    "standard-version": "^9.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
