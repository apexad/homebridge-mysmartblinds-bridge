{
  "name": "homebridge-mysmartblinds-bridge",
  "displayName": "Homebridge MySmartBlinds Bridge",
  "version": "2.1.0",
  "description": "Homebrige plugin for mysmartblinds (using hardware bridge)",
  "keywords": [
    "homebridge-plugin",
    "mysmartblinds",
    "category-hubs",
    "category-climate"
  ],
  "engines": {
    "node": ">=10.17.0",
    "homebridge": ">=1.0.0"
  },
  "dependencies": {
    "auth0": "^2.29.0",
    "jsonwebtoken": "^8.5.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@types/auth0-js": "^9.14.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/request-promise": "^4.1.46",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "homebridge": "^1.2.2",
    "nodemon": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "lint": "eslint src/**.ts --max-warnings=0",
    "lint:fix": "eslint src/**.ts --max-warnings=0 --fix",
